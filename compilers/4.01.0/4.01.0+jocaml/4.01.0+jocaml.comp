opam-version: "1"
version: "4.01.0"
src: "http://jocaml.inria.fr/pub/distri/jocaml-4.01/jocaml-4.01.0.tar.gz"
build: [
  ["./configure" "-prefix" prefix "-with-debug-runtime"]
